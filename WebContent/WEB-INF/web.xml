<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>studentManager</display-name>
  <servlet>
  	<servlet-name>login</servlet-name>
  	<servlet-class>studentManager.controller.LoginController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>login</servlet-name>
  	<url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>addStudent</servlet-name>
  	<servlet-class>studentManager.controller.AddStudentController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>addStudent</servlet-name>
  	<url-pattern>/addStudent</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>getStudent</servlet-name>
  	<servlet-class>studentManager.controller.GetStudentController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>getStudent</servlet-name>
  	<url-pattern>/getStudent</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>editStudent</servlet-name>
  	<servlet-class>studentManager.controller.EditStudentController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>editStudent</servlet-name>
  	<url-pattern>/editStudent</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>deleteStudent</servlet-name>
  	<servlet-class>studentManager.controller.DeleteStudentController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>deleteStudent</servlet-name>
  	<url-pattern>/deleteStudent</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>